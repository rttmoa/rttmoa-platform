{
  "name": "rttmoa-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "rttmoa-platform open source management system",
  "author": {
    "name": "rttmoa",
    "email": "rttmoa@gmail.com",
    "url": "https://github.com/rttmoa"
  },
  "license": "MIT",
  "homepage": "https://github.com/rttmoa",
  "repository": {
    "type": "git",
    "url": "git@github.com:rttmoa/rttmoa-platform.git"
  },
  "bugs": {
    "url": "https://github.com/rttmoa/rttmoa-platform/issues"
  },
  "scripts": {
    "start": "vite",
    "serve": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:test": "tsc && vite build --mode test",
    "build:pro": "tsc && vite build --mode production",
    "type:check": "tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "standard-version",
    "commit": "git status && git add -A && czg && git push"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@ant-design/pro-components": "^2.6.13",
    "@dnd-kit/core": "^6.0.8",
