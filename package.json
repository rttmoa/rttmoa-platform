    "@react-spring/core": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "@react-three/drei": "9.89.3",
    "@react-three/fiber": "^8.15.13",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-virtualized": "^9.21.7",
    "ahooks": "^3.7.8",
    "antd": "^5.8.4",
    "antd-img-crop": "^4.12.2",
    "axios": "^1.4.0",
    "braft-editor": "2.3.8",
    "cat-names": "^3.1.0",
    "clipboard": "^2.0.11",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.9",
    "draftjs-to-html": "^0.9.1",
    "draftjs-to-markdown": "^0.6.0",
    "echarts": "^5.4.3",
    "echarts-for-react": "^2.0.8",
    "echarts-liquidfill": "^3.1.0",
    "fs": "0.0.1-security",
    "highlight.js": "^11.8.0",
    "i18next": "^23.4.5",
    "jsencrypt": "^3.3.2",
    "lodash": "^4.17.21",
    "maath": "^0.10.7",
    "marked": "^9.0.3",
    "markmap-common": "^0.15.6",
    "markmap-lib": "^0.15.8",
    "markmap-view": "^0.15.8",
    "masonic": "^3.7.0",
    "md5": "^2.3.0",
    "mermaid": "^10.6.1",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "prism-react-renderer": "^2.3.1",
    "qrcode": "^1.5.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-countup": "^6.4.2",
    "react-cropper-pro": "^1.0.7",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.12.11",
    "react-draggable": "^4.4.5",
    "react-highlight-words": "^0.20.0",
    "react-i18next": "^13.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.4.3",
    "react-viewer": "^3.2.2",
    "react-virtualized": "^9.22.5",
    "react-zoom-pan-pinch": "^3.3.0",
    "recharts": "^2.8.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
